{
    "manifest_version": 3,
    "name": "Truth-Tides",
    "version": "1.0.0",
    "description": "Mitigating financial bias one tide at a time",
    "action": {
      "default_popup": "index.html"
    },
    "host_permissions": [
      "http://*/*",
      "https://*/*"
    ],
    "permissions": ["activeTab", "tabs", "scripting"]
}